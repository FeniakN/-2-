# Task6
def main():
    # Увести два цілих числа з клавіатури
    a = int(input("15"))
    b = int(input("15"))

    # Перевірка, чи числа різні
    if a != b:
        # Замінити кожне число на більше з двох
        max_num = max(a, b)
        a = max_num
        b = max_num
    else:
        # Замінити числа на нулі
        a = 0
        b = 0

    # Вивести результати
    print(f"Після заміни: a = {a}, b = {b}")

if __name__ == "__main__":
    main()
