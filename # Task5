# Task5
def main():
    # Увести координати точки А з клавіатури
    x = float(input("41 "))
    y = float(input("20"))

    # Перевірка розташування точки А
    if x == 0 and y == 0:
        print("Точка A знаходиться в початку координат.")
    elif x == 0:
        print("Точка A знаходиться на осі Y.")
    elif y == 0:
        print("Точка A знаходиться на осі X.")
    elif x > 0 and y > 0:
        print("Точка A знаходиться в першому квадранті.")
    elif x < 0 and y > 0:
        print("Точка A знаходиться в другому квадранті.")
    elif x < 0 and y < 0:
        print("Точка A знаходиться в третьому квадранті.")
    else:
        print("Точка A знаходиться в четвертому квадранті.")

if __name__ == "__main__":
    main()
